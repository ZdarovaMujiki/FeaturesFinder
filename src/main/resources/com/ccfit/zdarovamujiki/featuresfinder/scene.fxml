<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>

<?import javafx.scene.layout.GridPane?>
<GridPane xmlns:fx="http://javafx.com/fxml" alignment="CENTER" fx:controller="com.ccfit.zdarovamujiki.featuresfinder.controllers.FXMLController">
    <Label fx:id="placeLabel" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <TextField fx:id="placeInputField" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <Button fx:id="findButton" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <ListView fx:id="placesList" GridPane.columnIndex="0" GridPane.rowIndex="2" GridPane.columnSpan="2"/>
    <Label fx:id="weatherLabel" GridPane.columnIndex="2" GridPane.rowIndex="0" GridPane.rowSpan="2"/>
    <ListView fx:id="featureList" GridPane.columnIndex="2" GridPane.rowIndex="2" GridPane.rowSpan="2"/>
</GridPane>